def bubble_sort(list):
	n=len(list-1)
	for j in range(1,n):
		for i in range(1,n-j):
			if list[i-1]>list[i]:
				list[i-1],list[i]=list[i],list[i-1]




list=[9,8,7,10,11,23,6,5,4,3,2,0,1]
bubble_sort(list)
print(list)